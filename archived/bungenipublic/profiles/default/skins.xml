<?xml version="1.0"?>
<object name="portal_skins" default_skin="bungenipublic">
 
<object name="bungenipublic_images"
    meta_type="Filesystem Directory View"
    directory="Products.bungenipublic:skins/bungenipublic_images"/>
 <object name="bungenipublic_styles"
    meta_type="Filesystem Directory View"
    directory="Products.bungenipublic:skins/bungenipublic_styles"/>
 <object name="bungenipublic_templates"
    meta_type="Filesystem Directory View"
    directory="Products.bungenipublic:skins/bungenipublic_templates"/>
 <skin-path name="bungenipublic" based-on="Plone Default">
  <layer name="bungenipublic_styles"
     insert-after="custom"/>
  <layer name="bungenipublic_images"
     insert-after="bungenipublic_styles"/>
  <layer name="bungenipublic_templates"
     insert-after="bungenipublic_images"/>
 </skin-path>
</object>

