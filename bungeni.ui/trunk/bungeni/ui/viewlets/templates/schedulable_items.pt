<dt id="fieldsetlegend-..." 
    tal:attributes="id string:fieldsetlegend-${view/name}-for-scheduling"
    i18n:translate="" tal:content="view/name">
</dt>

<dd id="fieldset-..." tal:attributes="id string:fieldset-${view/name}-for-scheduling" class="items-for-scheduling">
    <table class="listing grid items-for-scheduling" id="items-for-scheduling" tal:condition="view/items"
        tal:attributes="id string:items-for-scheduling-${view/name}">
          <thead>
            <tr>
              <th>Type name</th>
              <th>Title</th>
              <th>Last modified</th>
            </tr>
          </thead>
          <tbody>      
            <tal:items repeat="item view/items">
              <tr tal:attributes="id string:row-${item/id}">
                <td tal:content="item/name" />
                <td>
                  <a rel="id"
                     tal:attributes="href item/url;
                                     name item/id;
                                     title item/description"
                     tal:content="item/title" />
                </td>
                <td i18n:translate="" tal:content="item/date" />
              </tr>
            </tal:items>
          </tbody>
    </table>
</dd>            
