<?xml version="1.0"?>
<configure xmlns="http://namespaces.zope.org/zope"
           xmlns:browser="http://namespaces.zope.org/browser" >

<!-- Bills -->           
  <browser:page
     name="view"
     for="bungeni.models.interfaces.IBill"
     class=".common.DisplayForm"
     permission="bungeni.bill.View"
     layer="bungeni.ui.interfaces.IBungeniSkin"
     />          

  <browser:page
     name="add"
     for="bungeni.models.interfaces.IBillContainer"
     class=".common.AddForm"
     permission="bungeni.bill.Add"
     layer="bungeni.ui.interfaces.IBungeniSkin"
     />          

  <browser:page
     name="edit"
     for="bungeni.models.interfaces.IBill"
