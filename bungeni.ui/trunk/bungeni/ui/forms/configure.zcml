  <!-- Expose form for use as macro -->
  
  <browser:page
     for="*"
     name="alchemist-form"
     permission="zope.Public"
     template="templates/form.pt"
     layer="bungeni.ui.interfaces.IBungeniSkin"
     />   

  <!-- Generic forms -->

  <browser:page
     name="view"
     for="ore.alchemist.interfaces.IAlchemistContent"
