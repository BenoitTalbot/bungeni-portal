<dt id="fieldsetlegend-offices-held" i18n:translate="">
  Offices held
</dt>

<dd id="fieldset-offices-held">
  <table class="listing">
    <thead>
      <tr>
        <th i18n:translate="office_held"> Office </th>
        <th i18n:translate="office_type"> Type </th>
        <th i18n:translate="office_title"> Title </th>        
        <th i18n:translate="office_from"> From </th>
        <th i18n:translate="office_to"> To </th>            
      </tr>
    </thead>
    <tbody>
      <tr tal:repeat="result view/offices_held"> 
        <tal:loop tal:repeat="data result">
          <td> <span tal:content="data" />  </td>
        </tal:loop>           
      </tr>
    </tbody>  
  </table>
</dd>
